  33                         ;    clrw    X          ; [5F] регистр X уже 
                               ;обнулил boot_F
  34                         ; $4817 ($000C)
  35                         boot_O_rx_wait_byte:  
  36  004817   5A                            decw     X               ; [5A]  
  37  004818   2716                          jreq     boot_O_exit     ; [27 16] по 
                                                                ;истечению 
                                                                ;таймаута выходим
                                                                ; из бутлоадера
  38  00481A X 720B0000F8                    btjf     UART1_SR, #5, boot_O_rx_wait_byte    ; [72 OB 52 30 F8]
  39                         ; первый байт принят, прекращаем 
