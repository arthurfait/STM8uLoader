                               ;используем как счетчик 
  40                         ; $481F ($0014)	
  41  00481F X C60000                        ld       A, UART1_DR                    ; [C6 52 31]
  42                         ; $4822 ($0017)	ждем очередной байт
  43                         boot_O_rx_wait_block:  
  44  004822 X 720B0000FB                    btjf     UART1_SR, #5, boot_O_rx_wait_block    ; [72 OB 52 30 FB]
