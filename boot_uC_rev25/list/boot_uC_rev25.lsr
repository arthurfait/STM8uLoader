  20                                   segment  byte at 4800 'boot_O_IMG'
  21                         ;0000FF00FF00FF00FF00FF350D523235
  22                         ;0C5235351452315A2716720B5230F8C6
  23                         ;5231720B5230FB3B52314A26F5965CFC
  24                         ;AE80042BFA90AE427FAE027FCC9FF600
