  28                         ; OPTION (RAM)
  29                         ; $480B ($0000)	образ начального 
                               ;загрузчика boot_OPTION
  30                         boot_O_start:  
  31                         ; настраиваем UART 96008N1 
                               ;Fmaster=16/8=2МГц/9600/16
  32                         ;    mov    UART1_BRR2, #0  ; [35 00 52 33]  
                               ;исключаем для экономии места
  33  00480B X 350D0000                      mov      UART1_BRR1, #13    ; [35 0D 52 32]
  34                         ; UART1_CR2.TEN <- 1  UART1_CR2.REN <- 1  разрешаем 
                               ;передачу/прием
  35  00480F X 350C0000                      mov      UART1_CR2, #%00001100       ; [35 0C 52 35]    
  36                         ; $4813 ($0008)	
  37                         boot_E_byte1_tx:  
  38                         ; отправляем версию $14 загрузчика
  39  004813 X 35140000                      mov      UART1_DR, #$14   ; [35 14 52 31]
  40                         ; это сигнал хост программе, что 
